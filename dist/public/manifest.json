  "shortcuts": [
    {
      "name": "Search Products",
      "short_name": "Search",
      "url": "/search",
      "description": "Search for products across all categories",
      "icons": [
        {
          "src": "/icons/shortcut-search.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "My Orders",
      "short_name": "Orders",
      "url": "/account/orders",
      "description": "Track and manage your orders",
      "icons": [
        {
          "src": "/icons/shortcut-orders.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Shopping Cart",
      "short_name": "Cart",
      "url": "/cart",
      "description": "View items in your shopping cart",
      "icons": [
        {
          "src": "/icons/shortcut-cart.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Flash Deals",
      "short_name": "Deals",
      "url": "/deals",
      "description": "Discover latest flash deals and offers",
      "icons": [
        {
          "src": "/icons/shortcut-deals.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "id": "com.getit.bangladesh",
      "url": "https://play.google.com/store/apps/details?id=com.getit.bangladesh"
    },
    {
      "platform": "itunes",
      "id": "1234567890",
      "url": "https://apps.apple.com/app/getit-bangladesh/id1234567890"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+getit",
      "url": "/product/%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
}